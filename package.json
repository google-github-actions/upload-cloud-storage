    "@types/sinon": "^10.0.20",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "@vercel/ncc": "^0.38.1",
    "chai": "^4.3.10",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint": "^8.52.0",
    "mocha": "^10.2.0",
